{
  "expo": {
    "name": "RAIDMASTER",
    "slug": "raidmaster",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.raidmaster.app"
    },
    "android": {
      "package": "com.raidmaster.app",
      "versionCode": 1,
      "permissions": [
        "VIBRATE"
      ]
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ]
    ]
  }
}